    "version": "0.2.0",
    "configurations": [
        {
            "name": "har2locust",
            "type": "python",
            "request": "launch",
            "program": "/usr/local/bin/har2locust",
            "console": "integratedTerminal",
            "args": [
                "tests/inputs/reqres.in.har",
                "-L",
                "DEBUG"
            ],
            "env": {
                "LC_ALL": "en_US.UTF-8"
            },
            "justMyCode": false,
            "gevent": true
        },
        {
            "name": "Python: current file",
            "type": "python",
            "gevent": true,
            "request": "launch",
            "program": "${file}",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "args": [],
            "justMyCode": false,
            "env": {
                "LOCUST_PLAYWRIGHT": "1",
                "LOCUST_TEST_ENV": "test"
            }
        }
    ]
}